{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    { "src": "/styles/(.*)", "dest": "/styles/$1" },
    { "src": "/scripts/(.*)", "dest": "/scripts/$1" },
    { "src": "/img/(.*)", "dest": "/img/$1" },
    { "src": "/(.*)", "dest": "/" }
  ]
}